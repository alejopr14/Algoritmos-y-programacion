Algoritmo Ejercicio7 
	Escribir " Escribe un numero: "
	Leer numero1
	
	contador<-0
	
	Para i<-1 Hasta numero1 Hacer
		si numero1 mod i = 0 Entonces
			contador<-contador+1
		FinSi
		
	FinPara
	
	Si contador=2 entonces 
		escribir " Es primo "
	SiNo
		Escribir " No es primo "
		
	FinSi
		
FinAlgoritmo

	
	
	
	
	
	
	
	
	
